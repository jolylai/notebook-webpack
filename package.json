{
  "private": true,
  "name": "webpack",
  "version": "1.0.0",
  "workspaces": [
    "examples/*"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.13",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.21",
    "prettier": "^1.19.1",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0",
    "yorkie": "^2.0.0"
  }
}
